{
    "name": "anas-t/caracal",
    "description": "Simple auth package.",
    "keywords": ["laravel", "authentication", "authorization", "auth"],
    "license": "MIT",
    "homepage": "https://github.com/AnasT/Caracal",
    "authors": [
        {
            "name": "Taha Anas",
            "email": "taha.anas09@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.*",
        "anas-t/mailing": "dev-master"
    },
    "require-dev": {
        "phpspec/phpspec": "2.0.0"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "files" : [
            "src/AnasT/Caracal/helpers.php"
        ],
        "psr-0": {
            "AnasT\\Caracal\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
